---
import Layout from "@layouts/Layout.astro";
import ScoreTable from "@components/ScoreTable.astro";

import { getCollection } from "astro:content";
const scores = await getCollection("scores", ({ id }) => {
  return id.startsWith('ukulele');
});
---

<Layout title="Ukulele Scores">
  <ScoreTable type="ukulele" scores={scores}/>
</Layout>
