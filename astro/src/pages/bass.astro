---
import Layout from "@layouts/Layout.astro";
import ScoreTable from "@components/ScoreTable.astro";

import { getCollection } from "astro:content";
const scores = await getCollection("scores", ({ id }) => {
  return id.startsWith('bass');
});
---

<Layout title="Bass Scores">
  <ScoreTable type="bass" scores={scores}/>
</Layout>
